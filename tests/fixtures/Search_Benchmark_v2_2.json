  "instructions": {
    "purpose": "Benchmark fixture for evaluating wine search identity matching (primary) and enrichment retrieval (secondary).",
    "runner_modes": {
      "replay": "Use recorded SERP snapshots; deterministic; ranking-only. Recommended for CI.",
      "record": "Fetch live SERP once and write snapshots for replay.",
      "live": "Run full pipeline (SERP + fetch + extraction). Use for nightly/manual runs; enforce live-only expectations."
    },
    "scoring": {
      "identity": {
        "primary_metrics": ["hit@1", "hit@3", "mrr"],
        "match_basis": "Normalized text match against gold_canonical_name; when present, vintage must match (or be NV)."
      },
      "enrichment": {
        "tracked_metrics": ["corroborated_claims_count", "sources_count", "latency_ms", "serp_calls", "pages_fetched", "bytes_downloaded"],
        "note": "Enrichment should be evaluated separately from identity to avoid flakiness across vintages and web changes."
      }
    },
    "case_fields": {
      "required": ["id", "query", "country", "challenges", "gold_canonical_name", "producer"],
      "optional": {
        "vintage": "Integer year if vintage is known. Omit for NV wines.",
        "expected": {
          "min_results": "Minimum number of results expected in live mode.",
          "must_include_domains": "List of domains that must appear among top results in live mode.",
          "must_include_source_types": "List of source types required in live mode (e.g., producer_website, critic, competition)."
        },
        "notes": "Human explanation of why this case exists / what it stresses."
      }
    },
    "challenge_taxonomy": {
      "name_complexity": ["long_name", "very_long_name", "short_query", "hyphenated_producer", "apostrophe", "definite_article"],
      "diacritics": ["diacritics_optional", "umlaut_optional", "special_chars_optional", "accented_grape"],
      "classification": ["classification_reserva", "classification_gran_reserva", "premier_cru", "pradikat_term", "gg_term", "gg_abbrev", "igt_tokens"],
      "vineyard": ["single_vineyard", "vineyard_name", "vineyard_tokens", "finca_keyword", "estate_name", "estate_token"],
      "brand_producer": ["brand_vs_producer", "producer_as_brand", "brand_only", "brand_line", "brand_family_range", "producer_acronym", "producer_small_footprint"],
      "numeric": ["numeric_bin", "numeric_cuvee"],
      "region": ["subregion_token", "appellation_token", "region_tokens"],
      "special_types": ["non_vintage", "super_tuscan", "appassimento_style", "prestige_champagne"],
      "search_difficulty": ["retail_noise", "many_retail_pages", "icon_wine", "icon_wine_short_query", "rare_high_value", "high_value", "low_critic_coverage"]
    }
  },
